@using DashboardDevaBNI.Component;
@using Microsoft.AspNetCore.Http;﻿

@{
    var SubDomain = GetConfig.AppSetting["Subdomain:Domain"];
    ViewData["Title"] = "Notice Of Payment";
}


@section Styles {
    <!-- DataTables -->
    <link rel="stylesheet" href="~/lib/adminlte-master/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/lib/adminlte-master/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
    <link rel="stylesheet" href="~/lib/adminlte-master/plugins/datatables-buttons/css/buttons.bootstrap4.min.css">

    <!-- Select2 -->
    <link rel="stylesheet" href="~/lib/adminlte-master/plugins/select2/css/select2.min.css">
    <link rel="stylesheet" href="~/lib/adminlte-master/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css">

    <!-- Upload File -->
    <link href="~/lib/dropify-master/dist/css/dropify.min.css" rel="stylesheet" />

    <!-- Toastr -->
    <link href="~/lib/adminlte-master/plugins/toastr/toastr.min.css" rel="stylesheet">

    <!-- Date -->
    <link href="~/lib/adminlte-master/plugins/daterangepicker/daterangepicker.css" rel="stylesheet">

}

@section Scripts {
    <!-- DataTables -->
    <script src="~/lib/adminlte-master/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/lib/adminlte-master/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/lib/adminlte-master/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/lib/adminlte-master/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
    <script src="~/lib/adminlte-master/plugins/datatables-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/lib/adminlte-master/plugins/datatables-buttons/js/buttons.bootstrap4.min.js"></script>

    <!-- DateFormat -->
    <script src="~/lib/adminlte-master/plugins/moment/moment.min.js"></script>

    <!-- Upload File -->
    <script src="~/lib/dropify-master/dist/js/dropify.min.js"></script>
    <script src="~/lib/adminlte-master/plugins/daterangepicker/daterangepicker.js"></script>


    <!-- Validation -->
    <script src="~/lib/adminlte-master/plugins/jquery-validation/jquery.validate.min.js"></script>
    <script src="~/lib/adminlte-master/plugins/jquery-validation/additional-methods.min.js"></script>

    <!-- Toastr -->
    <script src="~/lib/adminlte-master/plugins/toastr/toastr.min.js"></script>

    <!-- Sweetalert -->
    <script src="~/lib/bootstrap-sweetalert/sweetalert.min.js"></script>

    <!-- Select2 -->
    <script src="~/lib/adminlte-master/plugins/select2/js/select2.full.min.js"></script>

    @Html.Partial("_Script")
}


<div id="DivContentBody">
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Notice Of Payment</h1>
                </div>

                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item">
                            <a href="../Home/Index">Home</a>
                        </li>
                        <li class="breadcrumb-item active">Notice Of Payment</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <section class="col-lg-12">
                    <div class="row">
                        <div class="col-12">
                            <section class="content">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="card card-info">
                                            <div class="card-header btn" title="Collapse">
                                                <h5 style="float: left; font-weight: 600">Search</h5>
                                            </div>
                                            <div class="card-body">
                                                <div class="row">
                                                    <div class="col-sm-12 col-md-6">
                                                        <div class="form-group">
                                                            <label>Due Date : </label>
                                                            <div class="input-group">
                                                                <input type="text" class="form-control float-right" id="txtDueDateSearchParam">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text">
                                                                        <i class="far fa-calendar-alt"></i>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-12 col-md-6">
                                                        <div class="form-group">
                                                            <label>NOP No : </label>
                                                            <input id="txtNopNoSearchParam" type="search" class="form-control" placeholder="Type your NOP No here">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="card-footer">
                                                <div class="col-sm-12 ml-sm-auto">
                                                    <button class="btn btn-success" type="button" id="BtnSearch">
                                                        <i class="fas fa-search"></i>
                                                        Search
                                                    </button>
                                                    <button class="btn btn-danger" type="button" id="BtnClearSearch">
                                                        <i class="fas fa-undo"></i>
                                                        Clear
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>

                        <div id="DivTableCard" class="col-12">
                            <div class="card card-info">
                                <div class="card-body">
                                    <div class="table-container">
                                        <div class="margin py-3">
                                            <button type="button" class="btn btn-info" onclick="Create()"><i class="fas fa-plus"></i> Add Data</button>
                                            @* <button type="button" class="btn btn-info" onclick="CreateBulk()"><i class="fas fa-upload"></i> Add Data Bulk</button> *@
                                            <button type="button" class="btn btn-info" onclick="SendData()"><i class="fas fa-paper-plane"></i> Send Data / Check Status</button>
                                            <button type="button" class="btn btn-info" onclick="PrintPdfNoP()"><i class="fas fa-file-pdf"></i> Export PDF</button>
                                            <button type="button" class="btn btn-info" onclick="PrintExcelNoP()"><i class="fas fa-file-excel"></i> Export Excel</button>
                                        </div><div class="col">
                                            <div class="btn-group">
                                            </div>
                                        </div>
                                        <table id="Table" class="table table-bordered table-hover w-100">
                                            <thead>
                                                <tr>
                                                    <th style="width: 10px"><input type="checkbox" id="checkAll"></th>
                                                    <th style="width: 10px"></th>
                                                    <th style="width: 100px"></th>
                                                    <th></th>
                                                    <th></th>
                                                    <th></th>
                                                    <th></th>
                                                    <th></th>
                                                    <th></th>
                                                    <th></th>
                                                    <th></th>


                                                </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </section>
</div>
<div id="DivFormBody">
</div>
